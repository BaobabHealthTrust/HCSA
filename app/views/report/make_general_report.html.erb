   <HTML>
  <HEAD>
  <TITLE>General</TITLE>
  <style type = "text/css">


	#graph{
              position: relative;
   
   	      float: right;
	      padding: 0px 100px 0px 0px;
   
        }
	
	#plaintxt{
		margin-left: 7%;
		
	}

	#content{
		border: 5px solid black;
		width: 1250px;
		
	 }

   
  </style>
  </HEAD>
  <BODY>

<br><br>
<div id = "content">
<center><h4>THIS IS A GENERAL REPORT FROM <%=@start_date%> TO <%= @end_date%><h4></center>
 <br><br><br>

	<div id="graph">
        <h3>CHARTS</h3><br><br><br><br>
	<%= image_tag(@graph1) %>
         </div>	 

	<div id="plaintxt">       
	
     <h4>TOTAL VOTES (All Departments)   : <%= @prints.ceil %></h4>
         
     <h4>SATISFIED      :     <%= @prints1 %>   Percentage: <%= @percentage1.ceil %> </h4> 

     <h4>UNSATISFIED    :  <%= @prints2 %>      Percentage:   <%= @percentage2.ceil %></h4>

<table border = "2" width = "500">
	<tr>
		<th>SATISFACTION LEVEL</th>
		<th>NUMBER OF VOTES</th>
	</tr>
	
	<% @result2.each do |total| %>
	<tr>	
	<td><%= total.name%></td>
	<td><%= total.total_votes%></td>
	<% end %>
	</tr>
	
</table><br><br><br>

<table border = "2" width ="500">
	<tr>
		<th>CONCERN</th>
		<th>NUMBER OF VOTES</th>
	</tr>
	
	<% @concern2.each do |concern| %>
	<tr>	
	<td><%= concern.name%></td>
	<td><%= concern.total%></td>
	<% end %>
	</tr>		

</table><br><br>
	 <input type="button" value="Generate PDF" onClick="window.print()" />
   </div>
   </div>

 
  </BODY>
  </HTML>


