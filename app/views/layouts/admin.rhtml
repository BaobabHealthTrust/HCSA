		<td class="link" onmousedown="window.location='/settings/settingsMenu'" >Administration</td>
	      
	      <td class="link" onmousedown="window.location= '/votes/index'">Logout</td>
	    </tr>
	  </table>

	</div>
	
	
	<div class="content">
		<p><%= yield %></p>
	</div>
	
	<div class="footer">
		
	</div>

